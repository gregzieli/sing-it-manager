{
  "name": "sing-it-manager",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/bin/www",
    "build": "babel ./server --out-dir dist",
    "clean": "rimraf dist",
    "server": "nodemon --exec babel-node ./server/bin/www",
    "dev": "concurrently \"npm:server\" \"npm --prefix client start\"",
    "prod": "npm run clean && npm run build && npm start"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.18.2",
    "http-errors": "~1.6.3",
    "mongoose": "^5.8.11",
    "morgan": "~1.9.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "nodemon": "^2.0.2",
    "rimraf": "^3.0.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
